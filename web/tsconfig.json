{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["dom", "dom.iterable", "ES2022"],
		"module": "esnext",
		"moduleResolution": "bundler",
		"jsx": "preserve",
		"types": ["vite/client"],
		"paths": { "~/*": ["./src/*"] }
	},
	"include": ["**/*.ts", "**/*.tsx", "src/vite-env.d.ts"],
	"exclude": ["node_modules", "dist", "build", ".output", ".turbo"]
}
